<odoo>
    <record id="inherit_partner_form_view_strategic_page" model="ir.ui.view">

        <field name="name">partner.form.view.strategic.page</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='internal_notes']" position="after">
                <page name="strategic_account" string="Strategic Account">
                    <group>
                        <field name="strategic_account"/>
                        <field name="verified_strategic"/>
                        <field name="strategic_duration"/>
                        <field name="approval_date"/>
                        <field name="strategic_start_date"/>
                        <field name="strategic_end_date"/>
                    </group>

                </page>

            </xpath>

        </field>
    </record>

</odoo>
