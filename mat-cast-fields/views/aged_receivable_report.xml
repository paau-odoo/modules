    <record id="inherited_aged_receivable_report" model="account.report.column">
        <field name="report_id" ref="account_reports.aged_receivable_report" />
        <field name="name">Salesperson</field>
        <field name="sortable">True</field>
        <field name="expression_label">salesperson</field>
    </record>

    <record id="aged_receivable_line_salesperson" model="account.report.expression">
        <field name="report_line_id" ref="account_reports.aged_receivable_line" />
        <field name="label">salesperson</field>
        <field name="engine">custom</field>
        <field name="formula">_report_custom_engine_get_salesperson</field>
        <field name="subformula">salesperson</field>
        <field name="auditable" eval="False" />
    </record>


